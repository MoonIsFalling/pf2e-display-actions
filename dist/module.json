{
    "$schema": "https://json.schemastore.org/foundryvtt-manifest.json",
    "name": "pf2e-display-actions",
    "id": "pf2e-display-actions",
    "authors": [
        {
            "name": "Thieftain",
            "GitHub": "MoonIsFalling",
            "Discord": "Thieftain#0744"
        }
    ],
    "title": "Pf2e Display Actions",
    "description": "A small module to display the pathfinder action economy for convinience.",
    "version": "1.9.1",
    "packs": [],
    "system": [
        "pf2e"
    ],
    "scripts": [],
    "compatibility": {
        "minimum": "10",
        "verified": "10",
        "maximum": "10"
    },
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        }
    ],
    "styles": [
        "style.css"
    ],
    "esmodules": [
        "scripts/module.js"
    ],
    "url": "https://github.com/MoonIsFalling/pf2e-display-actions",
    "manifest": "https://github.com/MoonIsFalling/pf2e-display-actions/releases/download/1.9.1/module.json",
    "download": "https://github.com/MoonIsFalling/pf2e-display-actions/releases/download/1.9.1/module.zip",
    "includes": [
        "./images/**",
        "./languages/**",
        "./scripts/**",
        "./templates/**",
        "./module.json",
        "./README.md",
        "./*.css",
        "./*.css.map"
    ],
    "socket": true,
    "bugs": "https://github.com/MoonIsFalling/pf2e-display-actions/issues",
    "flags": {
        "allowBugReporter": true
    },
    "relationships": {
        "system": [
            "pf2e"
        ]
    }
}